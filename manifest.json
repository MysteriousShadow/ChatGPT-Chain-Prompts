{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "ChatGPT Chain Prompts - Free Automated Message Queue",
    "version": "1.1",
    "description": "Free and unlimited prompt chains for ChatGPT. Save, edit, and automatically queue prompts with custom separators.",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://chatgpt.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "128": "Logo.png"
    }
}